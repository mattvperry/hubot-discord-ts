{
  "name": "hubot-discord-ts",
  "version": "1.0.10",
  "description": "Hubot discord adapter written in Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "index.ts",
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "lint": "yarn eslint src --ext .js,.ts",
    "build": "yarn rollup -c",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattvperry/hubot-discord-ts.git"
  },
  "keywords": [
    "hubot",
    "discord"
  ],
  "author": "Matt Perry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattvperry/hubot-discord-ts/issues"
  },
  "homepage": "https://github.com/mattvperry/hubot-discord-ts#readme",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/scoped-http-client": "^0.11.0",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "discord.js": "discordjs/discord.js",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "hubot": "^3.3.2",
    "prettier": "^2.1.2",
    "rollup": "^2.28.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "discord.js": "discordjs/discord.js",
    "hubot": "^3.3.2"
  }
}